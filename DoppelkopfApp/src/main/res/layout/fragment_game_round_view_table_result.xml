<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_gravity="center" >


    <TextView
        android:id="@+id/fragment_game_round_view_table_round_bock_info"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/primary_dark"
        android:textSize="10sp"/>

    <TextView
        android:id="@+id/fragment_game_round_view_table_round_points"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="0"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/table_entry_round_points" />

    <TextView
        android:id="@+id/fragment_game_round_view_table_round_points_solo"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="0"
        android:textSize="12sp"
        android:textColor="@color/table_entry_round_bock_total" />




</LinearLayout>

